{
	"build": {
		"beforeDevCommand": "pnpm dev",
		"beforeBuildCommand": "pnpm build",
		"devPath": "http://localhost:1420",
		"distDir": "../dist",
		"withGlobalTauri": false
	},
	"package": {
		"productName": "WeChatAI",
		"version": "1.0.0"
	},
	"tauri": {
		"macOSPrivateApi": true,
		"allowlist": {
			"all": true
		},
		"bundle": {
			"active": true,
			"category": "DeveloperTool",
			"copyright": "Â© 2023 bingryan All Rights Reserved",
			"icon": [
				"icons/32x32.png",
				"icons/128x128.png",
				"icons/128x128@2x.png",
				"icons/icon.icns",
				"icons/icon.ico"
			],
			"identifier": "com.bingryan.WeChatAI",
			"longDescription": "Chat AI Model Application",
			"targets": "all"
		},
		"security": {
			"csp": null
		},
		"updater": {
			"active": false
		},
		"windows": [
			{
				"fullscreen": false,
				"resizable": true,
				"title": "",
				"width": 1080,
				"height": 600
			}
		],
		"systemTray": {
			"iconPath": "icons/icon.png",
			"iconAsTemplate": true
		}
	}
}